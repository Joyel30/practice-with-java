public class Main
 {
 	public static void main(String[] args) {
    int [] a={};
     int target = 2;
     int ans = linearsearch (a,target);
     System.out.println(ans);
 	}
	 	static int linearsearch(int [] arr,int target){
	   if(arr.length==0)
	    {
 	       return -1;
	        
 	    }
	    for(int i=0;i<arr.length;i++)
 	    if (target==arr[i]){
	        return i;
	    }
         return -1;
 	}


}
